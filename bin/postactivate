#!/bin/bash
# This hook is run after this virtualenv is activated.

# Create an environment variable to automatically select the 
# correct settings file. This means we can use django-admin.py
# from anywhere within out envirnoment instead of having to `cd`
# to the folder containing `manage.py`
#
# This file much be copied to the `bin/` folder of the virtualennv
export DJANGO_SETTINGS_MODULE=feral.settings.staging

